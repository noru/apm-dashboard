  "scripts": {
    "clean": "rimraf docs",
    "dev": "USE_MOCK=true node ./src/devServer.js",
    "dev:live": "USE_MOCK=false node ./src/devServer.js",
    "prod": "NODE_ENV=production node ./src/devServer.js",
    "prod:build": "npm run clean && npm run build && NODE_ENV=production node ./src/devServer.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./config/webpack.config.prod.js --progress",
    "build:fake": "npm run clean && cross-env NODE_ENV=production USE_MOCK=true webpack --config ./config/webpack.config.prod.js --progress",
    "build:legacy": "npm run --prefix src/legacy/react/reports dist",
    "build:deploy": "npm run build && sh push.sh dev && sh push.sh uat && sh push.sh sit",
    "coverage": "NODE_ENV=test nyc mocha --compilers ts:ts-node/register,tsx:ts-node/register,js:babel-core/register,css:./test/unit/cssNullCompiler.js -r tsconfig-paths/register ./test/unit/**/*.spec.*",
    "coverage:report": "NODE_ENV=test nyc --reporter=html mocha --compilers ts:ts-node/register,tsx:ts-node/register,js:babel-core/register,css:./test/unit/cssNullCompiler.js -r tsconfig-paths/register ./test/unit/**/*.spec.*",
    "e2e": "node ./test/e2e/runner.js",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' TS_NODE_PROJECT=./ NODE_ENV=test mocha --compilers ts:ts-node/register,tsx:ts-node/register,css:./test/unit/cssNullCompiler.js -r tsconfig-paths/register ./test/unit/**/*.spec.*",
    "testSingle": "NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' TS_NODE_PROJECT=./ mocha --compilers ts:ts-node/register,tsx:ts-node/register,css:./test/unit/cssNullCompiler.js -r tsconfig-paths/register ./test/unit/**/*$FILE*",
    "lint": "tslint -p tsconfig.json -c tslint.json --exclude './coverage/*' --exclude './config/*'"
  },
